<table>
    <thead>
    <tr>
        <th colspan="{{ divisionABTable.headingColspan }}">{{ divisionABTable.heading }}</th>
    </tr>
    </thead>
    <tbody>
    {% for row in divisionABTable.tableArray %}
        <tr>
            {% for col in row %}
                <td>{{ col }}</td>
            {% endfor %}
        </tr>
    {% endfor %}
    </tbody>
</table>